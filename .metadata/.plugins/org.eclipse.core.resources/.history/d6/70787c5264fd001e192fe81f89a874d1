package com.pran.JpaDemo;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Human 
{
	@Id
	private int hid;
	private String hname;
	private String htech;
	public int getHid() {
		return hid;
	}
	public void setHid(int hid) {
		this.hid = hid;
	}
	public String getHname() {
		return hname;
	}
	public void setHname(String hname) {
		this.hname = hname;
	}
	public String getHtech() {
		return htech;
	}
	public void setHtech(String htech) {
		this.htech = htech;
	}
	@Override
	public String toString() {
		return "Human [hid=" + hid + ", hname=" + hname + ", htech=" + htech + "]";
	}
	
	

}
